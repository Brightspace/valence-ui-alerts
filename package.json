{
  "name": "vui-alerts",
  "version": "0.1.4",
  "description": "Web component alert elements",
  "private": true,
  "scripts": {
    "prebuild": "cpy *.html ./bower_components/temp",
    "build": "vulcanize ./bower_components/temp/alert.html > ./test/perceptual/alert.html.ignore",
    "postbuild": "rimraf ./bower_components/temp",
    "preperceptual-diff": "npm run build",
    "perceptual-diff": "percy snapshot test/perceptual --enable_javascript",
    "postinstall": "bower install",
    "screenshot": "phantomjs ./screenshots/build.js",
    "test": "polylint && wct --skip-plugin sauce"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-alerts.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/valence-ui-alerts/issues"
  },
  "homepage": "https://github.com/Brightspace/valence-ui-alerts",
  "dependencies": {},
  "devDependencies": {
    "bower": "^1.7.7",
    "cpy-cli": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "polylint": "^2.10.0",
    "rimraf": "^2.3.4",
    "vulcanize": "^1.14.7",
    "web-component-tester": "^4.2.2"
  }
}
